{
    "include": [
        "./index.ts",
        "./index.test.ts",
        "./jest.config.ts",
        "./types/"
    ],
    "exclude": [],
    "compilerOptions": {
        "rootDir": ".",
        "outDir": ".",
        "typeRoots": [
            "./node_modules/@types/",
            "./types/"
        ],
        "baseUrl": "./",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        },
        "module": "ES6",
        "lib": [
            "dom",
            "esnext"
        ],
        "pretty": false,
        "importHelpers": true,
        "declaration": true,
        "sourceMap": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "esModuleInterop": true
    }
}
